package overloading;

public class Main02 {
	public static void main(String[] args) {
		// 알바생 둘을 만들어주세요.
		// 하나는 시급을 15000원으로 해주시고
		// 다른 하나는 입력 없이 생성해주세요.
		
		Parttimer p1 = new Parttimer(true,15000,"도미노피자","박기도");
		
		Parttimer p2 = new Parttimer(true,"롯데리아","김자바");
		p1.getInfo();
		p2.getInfo();
	}
}
