package accessmodifier;

public class Warrior extends Commoner {

	public Warrior(String name) {
		super(name);
		}
	public void hunt() {
		System.out.println("------------------");
		System.out.println("전사가 사냥을 시작합니다.");
		setHp(getHp() - 1);
		setExp(getExp() + 10);
	}
	public void doubleAttack() {
		System.out.println("--------------------");
		System.out.println("doubleAttack");
		setHp(getHp() - 4);
		setExp(getExp() + 25);
	}
	
}
